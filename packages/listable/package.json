    "directory": "packages/listable"
  },
  "bugs": {
    "url": "https://github.com/lerna-lite/lerna-lite/issues"
  },
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@qoopido/lerna.core": "workspace:*",
    "chalk": "^4.1.2",
    "columnify": "^1.6.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2"
