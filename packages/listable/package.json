{
  "name": "@qoopido/lerna.listable",
  "description": "Lerna-Lite listable utils",
  "version": "1.15.3-alpha.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.bundle.json --newLine LF",
